script/source = "extends RigidBody3D

@export var Camera : Node3D
@onready var camchange = false
@onready var correct = false
@onready var index 


func _ready():
	$AnimationPlayer.play(\"Idle\")
# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	index = DialogManager.current_line_index
	
	match index:
		0:
			
			pass
			#print(0)
		1:
			pass
			#print(1)
		2:
			get_tree().paused = true
			if Camera.size < 100:
				Camera.size += 0.5
			#print(2)
			#print(Camera.size)
			get_tree().paused = false

		3:
			#print(3)
			get_tree().paused = false
			$AnimationPlayer.play(\"Run\")
			await $AnimationPlayer.animation_finished
			Camera.follow = true
			self.queue_free()
			#print(\"Ending\")
			#print(Camera.size)

	
	
	match camchange:
		true:
			$Invisiwalls.scale = Vector3(1,1,1)
			Camera.size -= 0.3
			if Camera.size<20:
				Camera.follow = false
				camchange = false
				correct = true
		false:
			match correct:
				false:
					pass
				true:
					if Camera.position.x <= self.global_position.x:
						Camera.position.x += 0.1
						Camera.position.y = self.global_position.y+10
					else:
						correct = false



func _on_playertoggle_body_entered(body):
	if body.name == \"Player\":
		$Invisiwalls/Wall.play(\"Enter\")
		camchange = true


func _on_player_body_entered(body):
	pass # Replace with function body.
"

