script/source = "extends RigidBody3D
@onready var countbig = false
@onready var countsmall = false
@onready var start = 0
@onready var done = false

# Called when the node enters the scene tree for the first time.
func _ready():
	pass # Replace with function body.


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	match done:
		true:
			#print(\"done\")
			#print(GlobalVar.sizefactor)
			match GlobalVar.sizefactor:
				3.0:
					countbig = true
					#print(\"Bigcap\")
					
				0.5:
					countsmall = true
					#print(\"Smallcap\")
		false:
			pass

	if countbig == true and countsmall == true:
		start = 1

	match start:
		0:
			pass
		1:
			$AnimationPlayer.play(\"jUMP!\")
			var lines = $Dialogue_box2.lines
			DialogManager.start_dialog(global_position, lines)
			start = 2
		2: 
			pass

"

